{
  "name": "cybersec_ai_agent_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create cybersec_ai_agent_backend && dfx generate cybersec_ai_agent_backend && dfx deploy",
    "frontend": "vite --port 3000",
    "backend": "dfx deploy cybersec_ai_agent_backend --upgrade-unchanged",
    "start": "npm-run-all --parallel frontend backend-watch",
    "backend-watch": "nodemon --watch src/cybersec_ai_agent_backend -e rs --exec npm run backend",
    "dev": "npm run start",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\""
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/principal": "^2.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "assert": "^2.1.0",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "events": "^3.3.0",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.487.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "sass": "^1.63.6",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.1.3",
    "vite": "^6.2.2",
    "vite-plugin-environment": "^1.1.3"
  }
}
